{
	"name": "Nimble",
	"description": "You become even shorter when you sneak.",
	"type": "origins:action_over_time",
	"interval": 1,
	"rising_action": {
		"type": "origins:execute_command",
		"permission_level": 4,
		"command": "scale set pehkui:height 0.33 @s"
	},
	"falling_action": {
		"type": "origins:execute_command",
		"permission_level": 4,
		"command": "scale set pehkui:height 0.5 @s"
	},
	"condition": {
		"type": "origins:sneaking"
	}
}